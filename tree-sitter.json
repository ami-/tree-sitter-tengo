{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tengo",
      "camelcase": "Tengo",
      "title": "Tengo",
      "scope": "source.tengo",
      "file-types": [
        "tengo"
      ],
      "injection-regex": "^tengo$",
      "class-name": "TreeSitterTengo"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tengo grammar for tree-sitter",
    "authors": [
      {
        "name": "LT"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tengo"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}